config ipv6
	option 'policy_filter' '1'
	option 'managed' '0'

config ula
	option 'enable' 1
#	option 'net' 'fd43:e2b3:341c::/48'
	list 'nointerface' 'ge00'

config 6to4
	option 'enable' '0'

config 6in4
	option 'enable' '0'

config dhcpv6
	option 'enable' '0'
	option 'pd' '1'
	option 'distribute' '1'
	option 'pd_retain' '1'
#	option 'net' 'fd43:e2b3:341c::/48'

